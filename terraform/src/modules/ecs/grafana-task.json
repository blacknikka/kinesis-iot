[
    {
        "name": "grafana",
        "image": "${image_url}",
        "portMappings": [
            {
                "hostPort": 3000,
                "protocol": "tcp",
                "containerPort": 3000
            }
        ],
        "memoryReservation": 256,
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "${log_group_name}",
                "awslogs-region": "ap-northeast-1",
                "awslogs-stream-prefix": "${name}"
            }
        }
    }
]
